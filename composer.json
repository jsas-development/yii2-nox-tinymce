{
  "name": "nox-it/yii2-nox-tinymce",
  "description": "Yii2 TinyMCE Widget",
  "type": "library",
  "keywords": [
    "yii2",
    "extension",
    "migration",
    "framework",
    "php"
  ],
  "license": "BSD-3-Clause",
  "version": "1.0.2",
  "authors": [
    {
      "name": "Jonatas Sas",
      "email": "atendimento@jsas.com.br"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=5.4.0",
    "nox-it/yii2-nox": "1.*",
    "nox-it/yii2-nox-helpers": "1.*",
    "tinymce/tinymce": "4.3.*"
  },
  "autoload": {
    "psr-4": {
      "nox\\widgets\\tinymce\\": "widgets/tinymce"
    }
  },
  "config": {
    "process-timeout": 1800
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/nox-it/yii2-nox-tinymce"
    }
  ]
}
